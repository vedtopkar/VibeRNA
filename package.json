  "license": "MIT",
  "type": "module",
  "bin": {
    "ts-rna-draw": "./bin/ts-rna-draw.js"
  },
  "dependencies": {
    "bulma": "^0.9.1",
    "commander": "^2.20.3",
    "es6-promise": "^4.2.8",
    "interface": "^1.2.1",
    "paper": "^0.12.11",
    "parcel": "1.12.3",
    "sass": "^1.32.5",
    "structure": "^2.0.1",
    "tinycolor": "^0.0.1",
    "tinygradient": "^1.1.2",
    "typescript": "^4.1.3"
  },
  "scripts": {
