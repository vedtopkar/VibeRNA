{
    "name": "VibeRNA",
    "version": "0.2.0",
    "license": "MIT",
    "type": "module",
    "bin": {
        "VibeRNA": "./dist/cli/VibeRNA-CLI.js"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build --base /VibeRNA/",
        "doc": "typedoc --out dist/docs",
        "serve": "vite preview",
        "deploy": "yarn build && yarn doc && gh-pages -d dist",
        "lint": "eslint src --ext .ts,.vue"
    },
    "dependencies": {
        "commander": "^7.2.0",
        "postcss-import": "^14.0.1",
        "vue": "^3.0.5"
    },
    "devDependencies": {
        "@types/node": "^14.14.35",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.18.0",
        "@vitejs/plugin-vue": "^1.1.5",
        "@vue/compiler-sfc": "^3.0.5",
        "autoprefixer": "^10.2.5",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-vue": "^7.7.0",
        "gh-pages": "^3.1.0",
        "husky": "=4",
        "lint-staged": "^10.5.4",
        "prettier": "^2.2.1",
        "tailwindcss": "^2.0.3",
        "typedoc": "^0.20.33",
        "typescript": "^4.1.3",
        "vite": "^2.0.5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{ts,vue}": "eslint",
        "*": [
            "prettier --write --ignore-unknown",
            "git add"
        ]
    }
}
